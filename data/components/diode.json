{

	"displayNameBase": "Diode",
	"letterIdentifierBase": "D",

	"nameposition": [ 0, -12 ],

	"DCOP": {

		"linear": false,

		  "componentStamps": [ 

			{

				"matrix": "A",

				"row":  "this.node1.voltageIndex",
				"column":  "this.node1.voltageIndex", 

				"value": "(this.properties.sat_curr / this.properties.therm_volt) * exp((this.node1.voltageValue - this.node0.voltageValue) / this.properties.therm_volt)"

			},

			{

				"matrix": "A",

				"row":  "this.node1.voltageIndex",
				"column":  "this.node0.voltageIndex", 

				"value": "-1 * (this.properties.sat_curr / this.properties.therm_volt) * exp((this.node1.voltageValue - this.node0.voltageValue) / this.properties.therm_volt)"

			},

			{

				"matrix": "A",

				"row":  "this.node0.voltageIndex",
				"column":  "this.node1.voltageIndex", 

				"value": "-1 * (this.properties.sat_curr / this.properties.therm_volt) * exp((this.node1.voltageValue - this.node0.voltageValue) / this.properties.therm_volt)"

			},

			{

				"matrix": "A",

				"row":  "this.node0.voltageIndex",
				"column":  "this.node0.voltageIndex", 

				"value": "(this.properties.sat_curr / this.properties.therm_volt) * exp((this.node1.voltageValue - this.node0.voltageValue) / this.properties.therm_volt)"

			},

			{

				"matrix": "B",

				"row":  "this.node0.voltageIndex",

				"value": "(this.node1.voltageValue - this.node0.voltageValue) * (this.properties.sat_curr / this.properties.therm_volt) * exp((this.node1.voltageValue - this.node0.voltageValue) / this.properties.therm_volt) - this.properties.sat_curr * (exp((this.node1.voltageValue - this.node0.voltageValue) / this.properties.therm_volt) - 1)"

			},

			{

				"matrix": "B",

				"row":  "this.node1.voltageIndex",

				"value": "-(this.node1.voltageValue - this.node0.voltageValue) * (this.properties.sat_curr / this.properties.therm_volt) * exp((this.node1.voltageValue - this.node0.voltageValue) / this.properties.therm_volt) + this.properties.sat_curr * (exp((this.node1.voltageValue - this.node0.voltageValue) / this.properties.therm_volt) - 1)"

			}

		]

	},

	"properties": [

		{
			"name": "sat_curr",
			"displayName": "Saturation current",
			"unit": "A",
			"defaultValue": 1e-14
		},
		{
			"name": "therm_volt",
			"displayName": "Thermal voltage",
			"unit": "V",
			"defaultValue": 25.8e-3
		},
		{
			"name": "n",
			"displayName": "Quality factor",
			"unit": "",
			"defaultValue": 1
		}

	],	

